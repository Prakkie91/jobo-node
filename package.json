{
  "name": "jobo-enterprise",
  "version": "2.0.0",
  "description": "Official Node.js/TypeScript client for the Jobo Enterprise API. Access millions of job listings from 45+ ATS platforms, geocode locations, and automate job applications. Organized by feature area: Feed, Search, Locations, and Auto Apply.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "jobo",
    "jobs",
    "api",
    "enterprise",
    "job-board",
    "recruitment",
    "ats",
    "greenhouse",
    "lever",
    "workday",
    "smartrecruiters",
    "ashby",
    "bamboohr",
    "icims",
    "job-scraper",
    "job-listings",
    "job-data",
    "job-aggregator",
    "job-feed",
    "applicant-tracking-system",
    "hiring",
    "careers-page",
    "job-search-api",
    "bulk-jobs"
  ],
  "author": "Jobo <support@jobo.world>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Prakkie91/jobo-node.git"
  },
  "homepage": "https://jobo.world/enterprise/",
  "bugs": {
    "url": "https://github.com/Prakkie91/jobo-node/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0"
  }
}
